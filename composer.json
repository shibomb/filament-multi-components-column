{
    "name": "shibomb/filament-multi-components-column",
    "description": "A Simple Mutli Components Column for Filament Table",
    "keywords": [
        "shibomb",
        "laravel",
        "filament-multi-components-column",
        "multi",
        "components",
        "inline-flex"
    ],
    "homepage": "https://github.com/shibomb/filament-multi-components-column",
    "license": "MIT",
    "authors": [
        {
            "name": "shibomb",
            "email": "shibomb@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "filament/filament": "^2.0",
        "illuminate/contracts": "^8.0|^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10|^6.1",
        "orchestra/testbench": "^6.23|^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.0|^9.5"
    },
    "autoload": {
        "psr-4": {
            "Shibomb\\FilamentMultiComponentsColumn\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shibomb\\FilamentMultiComponentsColumn\\Tests\\": "tests",
            "Shibomb\\FilamentMultiComponentsColumn\\Tests\\Database\\Factories\\": "tests/database/factories"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shibomb\\FilamentMultiComponentsColumn\\MultiComponentsColumnServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
